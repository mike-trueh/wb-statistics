{
  "name": "mike-trueh/wb-stat",
  "description": "SDK for working with the Wildberries statistics API",
  "minimum-stability": "alpha",
  "keywords": [
    "wildberries",
    "wb.ru",
    "sdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Kryzhanovskiy",
      "email": "mike.trueh@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "WbStat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": [
        "tests/"
      ]
    }
  }
}
